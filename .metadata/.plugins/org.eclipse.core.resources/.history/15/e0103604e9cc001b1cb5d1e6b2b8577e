package net.sf.antcontrib.util;

import java.io.IOException;

public class Utils {

	public static void close(InputStream is) {
		try {
			if (is != null) {
				is.close();
			}
		}
		catch (IOException e) {
			;
		}
		
	}

	public static void close(OutputStream is) {
		try {
			if (is != null) {
				is.close();
			}
		}
		catch (IOException e) {
			;
		}
		
	}
}
